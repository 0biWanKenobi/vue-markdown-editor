{
  "name": "@kangc/v-md-editor",
  "version": "0.0.8",
  "description": "A lightweight markdown editor built on Vue",
  "main": "lib/index.js",
  "unpkg": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "dev": "npm run build:entry && webpack-dev-server --config build/webpack.dev.js",
    "lint": "eslint ./src --ext .js,.vue,.ts,.tsx --fix && stylelint \"src/**/*.vue\" --fix",
    "build:entry": "node build/build-entry.js",
    "build:utils": "babel src/utils --out-dir lib/utils",
    "build:pkg": "npm run build:entry && webpack --config build/webpack.pkg.js && npm run build:utils",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "release": "node build/release.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "git add"
    ],
    "*.{vue,css,scss}": [
      "stylelint",
      "git add"
    ]
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vant/eslint-config": "^2.0.0",
    "@vant/stylelint-config": "^1.3.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.3",
    "inquirer": "^7.1.0",
    "lint-staged": "^10.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.3",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "semver": "^7.1.3",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vuepress": "^1.4.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-farmer-i/vue-markdown-editor.git"
  },
  "keywords": [
    "vue",
    "markdown",
    "editor"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-farmer-i/vue-markdown-editor/issues"
  },
  "homepage": "https://github.com/code-farmer-i/vue-markdown-editor#readme",
  "dependencies": {
    "codemirror": "^5.52.2",
    "core-js": "^3.6.4",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^9.18.1",
    "markdown-it": "^10.0.0",
    "prismjs": "^1.20.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
