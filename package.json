{
  "name": "@kangc/v-md-editor",
  "version": "2.3.13-beta.1",
  "description": "A markdown editor built on Vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-farmer-i/vue-markdown-editor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-farmer-i/vue-markdown-editor/issues"
  },
  "homepage": "https://github.com/code-farmer-i/vue-markdown-editor#readme",
  "main": "lib/main.js",
  "keywords": [
    "javascript",
    "vue3",
    "vue",
    "html",
    "markdown",
    "editor",
    "vue-markdown-editor",
    "vue3-markdown-editor",
    "vue-markdown",
    "vue3-markdown",
    "markdown-it",
    "vue-editor",
    "vue3-editor",
    "markdown-editor"
  ],
  "unpkg": "lib/base-editor.js",
  "files": [
    "src",
    "lib",
    "es",
    "types"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "serve": "vue-cli-service serve dev/main.ts",
    "build": "vue-cli-service build --modern && npx vue-tsc --project prod.tsconfig.json",
    "build:cli-orig": "vue-cli-service build --target lib --name VMdEditor src/main.ts",
    "build:theme": "webpack --config build/webpack.theme.js",
    "build:pkg": "webpack --config build/webpack.pkg.js && npm run build:theme && npm run build:utils",
    "lint-staged": "lint-staged",
    "lint": "eslint ./src --ext .js,.vue,.ts,.tsx --fix && stylelint \"src/**/*.vue\" --fix",
    "test-cdn-pkg": "http-server -o ./dev/test-cdn.html",
    "test-cdn-cli": "http-server -o ./dev/test-cdn-cli.html",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write"
    ],
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{vue,css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.2.21",
    "vue": "^3.2.21"
  },
  "resolutions": {
    "acorn": "npm:acorn-with-stage3"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@vuepress/markdown": "^1.8.2",
    "@vuepress/shared-utils": "^1.9.5",
    "codemirror": "^5.61.1",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.20.2",
    "highlight.js": "^10.7.2",
    "insert-text-at-cursor": "^0.3.0",
    "katex": "^0.13.11",
    "markdown-it": "^12.0.6",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-container": "^3.0.0",
    "mermaid": "^8.10.1",
    "prismjs": "^1.23.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.2.26",
    "vue-loader": "^17.0.0",
    "vue-types": "^4.1.1",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/codemirror": "^5.60.5",
    "@types/insert-text-at-cursor": "^0.3.0",
    "@types/katex": "^0.11.1",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/markdown-it-container": "^2.0.4",
    "@types/markdown-it-emoji": "^2.0.2",
    "@types/mermaid": "^8.2.7",
    "@types/node": "^17.0.6",
    "@types/prismjs": "^1.16.6",
    "@types/webpack-chain": "^5.2.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.26",
    "acorn-with-stage3": "^1.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.10.0",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.3",
    "less": "^4.1.2",
    "less-loader": "7.3.0",
    "lint-staged": "^10.1.2",
    "module": "^1.2.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.3",
    "sass": "^1.47.0",
    "sass-loader": "^10",
    "source-map": "^0.7.3",
    "stylus": "^0.56.0",
    "typescript": "^4.5.4",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "postinstall": "patch-package"
}
