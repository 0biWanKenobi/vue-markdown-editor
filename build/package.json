    "@rollup/plugin-typescript": "^8.3.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/bluebird": "^3.5.36",
    "@types/codemirror": "^5.60.5",
    "@types/insert-text-at-cursor": "^0.3.0",
    "@types/katex": "^0.11.1",
    "@types/markdown-it-attrs": "^4.1.0",
