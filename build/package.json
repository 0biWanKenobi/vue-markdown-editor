{
  "name": "@kangc/v-md-editor",
  "version": "3.0.0",
  "description": "A markdown editor built on Vue 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-farmer-i/vue-markdown-editor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-farmer-i/vue-markdown-editor/issues"
  },
  "homepage": "https://github.com/code-farmer-i/vue-markdown-editor#readme",
  "types": "types/index.esm.d.ts",
  "files": [
    "*.cjs",
    "package.json",
    "*.css",
    "font/**/*"
  ],
  "main": "./index.cjs",
  "module": "./index.esm.mjs",
  "exports": {
    ".": {
      "import": {
        "node": "./index.esm.mjs",
        "default": "./index.cjs"
      },
      "require": "./index.cjs",
      "types": "./types/index.esm.d.ts"
    },
    "./editor": {
      "import": "./editor/index.esm.mjs"
    },
    "./preview": {
      "import": "./preview/index.esm.mjs"
    },
    "./theme": {
      "import": "./theme/index.esm.mjs"
    },
    "./lang": {
      "import": "./lang/index.esm.mjs"
    },
    "./typings": {
      "import": "./typings/index.esm.mjs"
    },
    "./package.json": {
      "import": "./package.json"
    }
  },
  "keywords": [
    "javascript",
    "vue3",
    "vue",
    "html",
    "markdown",
    "editor",
    "vue-markdown-editor",
    "vue3-markdown-editor",
    "vue-markdown",
    "vue3-markdown",
    "markdown-it",
    "vue-editor",
    "vue3-editor",
    "markdown-editor"
